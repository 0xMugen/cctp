{
	"name": "cctp",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "NODE_OPTIONS=--max-old-space-size=4096 svelte-kit sync && NODE_OPTIONS=--max-old-space-size=4096 svelte-check --tsconfig ./tsconfig.check.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "NODE_OPTIONS=--max-old-space-size=4096 prettier --check . && NODE_OPTIONS=--max-old-space-size=4096 eslint --cache --cache-location .eslintcache --ext .ts,.js,.svelte src scripts",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"migrate": "tsx scripts/migrate.ts",
		"migrate:up": "tsx scripts/migrate.ts up",
		"migrate:down": "tsx scripts/migrate.ts down",
		"migrate:status": "tsx scripts/migrate.ts status",
		"migrate:dry-run": "tsx scripts/migrate.ts dry-run",
		"migrate:generate-schema": "tsx scripts/migrate.ts generate-schema"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/pg": "^8.15.4",
		"@vitest/browser": "^3.2.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"playwright": "=1.52.0",
		"prettier": "3.6.2",
		"prettier-plugin-svelte": "3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "5.36.6",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"tsx": "^4.20.3",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vite-plugin-devtools-json": "^0.2.0",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"dependencies": {
		"@solana/spl-token": "^0.4.0",
		"@solana/web3.js": "^1.98.0",
		"@starknet-io/get-starknet": "^4.0.8",
		"@wagmi/connectors": "^7.1.1",
		"@wagmi/core": "^3.2.1",
		"dotenv": "^17.2.0",
		"ethers": "^6.13.0",
		"pg": "^8.16.3",
		"starknet": "^6.0.0",
		"svelte-wagmi": "^1.0.7",
		"viem": "^2.0.0"
	}
}
